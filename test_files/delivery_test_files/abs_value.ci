program absolutevalue

	declare x;
	
	function absvalue(in x)
	{
		forcase
			case (x >= 0) x := x;
			case (x < 0) x := - x;
			default
				;
		
	}
	# m a i n #
	{
		input(x);
		print(absvalue(in x));
	}. 