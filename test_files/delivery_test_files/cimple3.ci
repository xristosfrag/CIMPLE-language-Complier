program pow

    declare N,num,temp;

    #main#
    {
        input(num);
        input(N);
        temp := num;

        if(N <> 0)
        {
            if(N < 0)
            {
                N := N * (-1);
            };
           
            while(N>1)
            {
                num := num * temp;
                N := N - 1;
            };

            print(num);
            
        }
        else{
            print (1);
        }
    }.