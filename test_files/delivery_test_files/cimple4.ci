# ROCK = 1 #
# SCIRRORS = 2 #
# PAPER = 3 #
# enter your name as integer #

program rockScissorsPaper

    declare u1,u2,user1,user2, itsatie;

    #main#
    {
        itsatie := 0;
        input(user1);
        input(user2);

        input(u1);
        input(u2);

        if (u1 = u2){
           print(itsatie)
        }
        else{
            if(u1 = 1){
                if (u2 = 2){
                    print(user1);
                }else{
                    if(u2 = 3){
                        print(user2);
                    }else{
                        print(-1);
                    }
                }
            }else{
                if (u1 = 2){
                    if (u2 = 3){
                        print(user1);
                    }else{
                        if(u2 = 1){
                            print(user2);
                        }else{
                            print(-1);                            
                        }
                    }
                }else{
                    if (u1 = 3){
                        if (u2 = 1){
                            print (user1);
                        }else{
                            if(u2 = 2){
                                print(user2);
                            }else{
                                print(-1);
                            }
                        }
                    }
                    else{
                        print(-1);
                    }
                }
            }
        }
    }.