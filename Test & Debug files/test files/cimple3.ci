program triangle
    
    declare givenNumber, result;
    
    function createTriangle(in s)

        declare row, column;
    {
        row := 1;
        column := 1;

        forcase
            case(row<s)
            {
                row := row + 1;

                forcase
                    case(column<row)
                    {
                        print(column);
                        column := column + 1;
                    };
                    default
                        ;
            };
            default
                print(x);
    };

    #main#
    {
        input(givenNumber);
        result := createTriangle(in givenNumber)
    }.