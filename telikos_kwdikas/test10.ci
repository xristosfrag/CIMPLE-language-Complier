program MAX

    declare a,b,c,d,e;

    function max(in x, in y)
        if(x>y)
            return (x);
        else
            return(y);
        ;
    
{
    input(a);
    input(b);
    input(c);
    input(d);
    input(e);
    e := max(in max(in max(in a, in b), in c), in max(in d, in e));
    print(e);
}.